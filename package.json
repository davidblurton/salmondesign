    "type": "git",
    "url": "http://github.com/rthor/gulp-base.git"
  },
  "author": {
    "name": "Ragnar Þór Valgeirsson (rthor)",
    "email": "ragnar.valgeirsson@gmail.com",
    "url": "http://rthor.is"
  },
  "bugs": {
    "url": "https://github.com/rthor/gulp-base/issues"
  },
  "homepage": "https://github.com/rthor/gulp-base",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^9.0.3",
    "browserify-shim": "^3.8.3",
    "glob": "^4.4.1",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-changed": "^1.1.1",
    "gulp-connect": "^2.2.0",
    "gulp-csso": "^1.0.0",
    "gulp-imagemin": "^2.2.1",
    "gulp-jade": "^1.0.0",
