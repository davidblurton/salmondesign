  "license": "MIT",
  "devDependencies": {
    "bower": "^1.3.3",
    "browserify": "^4.2.0",
    "browserify-shim": "^3.6.0",
    "glob": "^4.0.4",
    "gulp": "^3",
    "gulp-autoprefixer": "^0.0.8",
    "gulp-changed": "^0.4.0",
