    "gulp-rename": "^1.2.0",
    "gulp-util": "^3.0.4",
    "gulp-watch": "^4.1.1",
    "rimraf": "^2.3.0",
    "run-sequence": "^1.0.2",
    "vinyl-source-stream": "^1.0.0"
  },
  "dependencies": {
    "jquery": "^2.1.3",
    "normalize.css": "^3.0.2"
  }

}
